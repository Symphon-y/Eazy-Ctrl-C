{
   "manifest_version": 3,
   "name": "EzApply",
   "version": "0.01",
   "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "index.html"
  },
  "icons": {
    "16": "favicon.png"
  }
}